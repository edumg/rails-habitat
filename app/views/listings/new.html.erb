
 <div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">

      <div class="panel panel-primary">
        <div class="panel-heading text-center">New Listing</div>
        <div class="panel-body">

          <%= render 'form' %>

        </div>

      </div>
      <%= link_to listings_path do %>
      <i class="fa fa-chevron-left fa-4x"></i>
      <% end %>
    </div>


  </div>

</div>

</div>

<div class="container padded-top">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= simple_form_for(@listing) do |f| %>
        <%= f.input :location %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyDANjx3bosEtIyzJaoWs50Wnt6nt_1rmxU"></script>
<% end %>
