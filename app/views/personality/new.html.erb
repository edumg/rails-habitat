<div class="wrap" style="padding-top: 0px">
  <div class="fleft", style="width: 56%">
    <div class="title_empty">
     <div>
       <div class="container", style="padding-left: -100px">
         <h1>Welcome!</h1>
       </div>
       <div class="container">
         <h2><strong>Let's get to know you better.</strong> <%= link_to "Why we do this test?", new_personality_path, class: "btn btn-primary", style: "font-size: 9px; border-radius: 20px" %></h2>
       </div>

       <div class="container">
         <div class="col-xs-6 col-sm-6 col-md-12">
           <% @questions.each do |question| %>
             <%= link_to :anchor => "wall" do %>
               <div class="col-xs-6 col-sm-6 col-md-1", style="margin-right: 25px; display:block">
                 <h6 style="width: 100px">Question <%= question.id %></h6>
                 <%= link_to "",  new_personality_path, class: "btn btn-primary", style: "heigth: 1px; border-radius: 30px; width: 100px" %>
               </div>
             <% end %>
           <% end %>
         </div>
       </div>

       <br>

       <div class="container">
         <div class="col-xs-6 col-sm-4 col-md-6">
           <%= cl_image_tag "party_nv0ts7", class: "personality_photo", id:"persona_img_1"%>
           <%= cl_image_tag "john-sting-112628_urpf9v", class: "personality_photo", id:"persona_img_2"%>
           <%= cl_image_tag "File1_txtgha", class: "personality_photo", id:"persona_img_3" %>
           <%= cl_image_tag "File3_azp3hb", class: "personality_photo", id:"persona_img_4" %>
           <%= cl_image_tag "File2_uxpdvw", class: "personality_photo", id:"persona_img_5" %>
           <%= cl_image_tag "File4_lmvfil", class: "personality_photo", id:"persona_img_6" %>
          <%= link_to "Next", root_path, class: "btn btn-primary hidden", id: "button-next"  %>
         </div>


       </div>

     </div>

   </div>
 </div>

 <div class="fright", style="width: 44%">
  <div class="banner" style="background: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(60,60,80,0.6) 20%), url('<%= cl_image_path "room_if6zlj" %>');">
  </div>
</div>
</div>
