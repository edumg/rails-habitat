<div class="container">
  <div class="row row-marging">
    <h2 class="text-center marging-top">
      Available listings
    </h2>
    <h3 class="text-center margin-bottom">
      <%= link_to  listings_path do %>
            <strong><%= Listing.count %> listings</strong> available in Amsterdam
          <% end %>
    </h3>
    <div class="row">
      <div class="col-xs-12 col-sm-6">
        <div class="row">
          <div class="container">
            <% @listings.each do |listing| %>
            <%= render partial: 'card', locals: { listing: listing } %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
